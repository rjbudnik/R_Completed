CF.F4.LSD<-CF.F4.lsd[["LSD"]][["Aveg.LSD"]]
## Location 4
CF.F5.Mean<-mean(C.F.LSM5$F5)
CF.F5.Min<-min(C.F.LSM5$F5)
CF.F5.Max<-max(C.F.LSM5$F5)
