LSM.S_Full.skldg<-as.data.frame(emmeans(model.S_Full.skldg, spec="code"))
LSM.S_Full.skldg<-LSM.S_Full.skldg%>%select(code, emmean)
names(LSM.S_Full.skldg)[2]<-c("SF.skldg")
LSM.S_Full.skldg$SF.skldg[LSM.S_Full.skldg$SF.skldg<0] <- 0
```

## Merge Lodging Scores

```{r merge.lodging, comment=NA, echo=FALSE, message=FALSE}
## Merge
## NORTH
NE.ldg<-left_join(LSM.N_Early.rtldg, LSM.N_Early.skldg)
NF.ldg<-left_join(LSM.N_Full.rtldg, LSM.N_Full.skldg)
## CENTRAL
CE.ldg<-left_join(LSM.C_Early.rtldg, LSM.C_Early.skldg)
CF.ldg<-left_join(LSM.C_Full.rtldg, LSM.C_Full.skldg)
## SOUTH
SE.ldg<-left_join(LSM.S_Early.rtldg, LSM.S_Early.skldg)
SF.ldg<-left_join(LSM.S_Full.rtldg, LSM.S_Full.skldg)
```



# District Merge & Rank

```{r Merge.Rank, comment=NA, echo=FALSE, message=FALSE}
## Combine EL/CS & E/W LSMs:
## NORTH
NE.Combo<-left_join(NE.Combo, NE.Split.ew)
NF.Combo<-left_join(NF.Combo, NF.Split.ew)
## CENTRAL
CE.Combo<-left_join(CE.Combo, CE.Split.ew)
CF.Combo<-left_join(CF.Combo, CF.Split.ew)
## SOUTH
SE.Combo<-left_join(SE.Combo, SE.Split.ew)
SF.Combo<-left_join(SF.Combo, SF.Split.ew)

## Combine COMBO directly above with Lodging:
## NORTH
NE.Combo<-left_join(NE.Combo, NE.ldg)
NF.Combo<-left_join(NF.Combo, NF.ldg)
## CENTRAL
CE.Combo<-left_join(CE.Combo, CE.ldg)
CF.Combo<-left_join(CF.Combo, CF.ldg)
## SOUTH
SE.Combo<-left_join(SE.Combo, SE.ldg)
SF.Combo<-left_join(SF.Combo, SF.ldg)
```

# Subset 2 Year Matches & Rank

```{r subset.only.2.year.matches.and.rank, echo=FALSE}
## Subset codes that appear in previous year:
# Use these!
NE.2yr<-subset(NE.Combo, code %in% P.N_Early$code)
NF.2yr<-subset(NF.Combo, code %in% P.N_Full$code)
CE.2yr<-subset(CE.Combo, code %in% P.C_Early$code)
CF.2yr<-subset(CF.Combo, code %in% P.C_Full$code)
SE.2yr<-subset(SE.Combo, code %in% P.S_Early$code)
SF.2yr<-subset(SF.Combo, code %in% P.S_Full$code)

## ADD Rankings:
NE.2yr$yld.rank<-as.numeric(rank(-NE.2yr$ls.yld))
NF.2yr$yld.rank<-as.numeric(rank(-NF.2yr$ls.yld))
CE.2yr$yld.rank<-as.numeric(rank(-CE.2yr$ls.yld))
CF.2yr$yld.rank<-as.numeric(rank(-CF.2yr$ls.yld))
SE.2yr$yld.rank<-as.numeric(rank(-SE.2yr$ls.yld))
SF.2yr$yld.rank<-as.numeric(rank(-SF.2yr$ls.yld))
```


# Re-Arrange & Sort

```{r Rearrange.Sort, comment=NA, echo=FALSE, message=FALSE}
## NORTH
## North Early
NE.2yr<-NE.2yr[,c(1,2,6,8,9,10,17,11,14,13,16,15,12)]
## North Full
NF.2yr<-NF.2yr[,c(1,2,6,8,9,10,17,11,14,13,16,15,12)]

## CENTRAL
## Central Early
CE.2yr<-CE.2yr[,c(1,2,6,8,9,10,17,11,14,13,16,15,12)]
## Central Full
CF.2yr<-CF.2yr[,c(1,2,6,8,9,10,17,11,14,13,16,15,12)]

## SOUTH
## South Early
SE.2yr<-SE.2yr[,c(1,2,6,8,9,10,17,11,14,13,16,15,12)]
## South Full
SF.2yr<-SF.2yr[,c(1,2,6,8,9,10,17,11,14,13,16,15,12)]


## Sort By Rank:
NE.2yr<-NE.2yr[order(NE.2yr$yld.rank),]
NF.2yr<-NF.2yr[order(NF.2yr$yld.rank),]
CE.2yr<-CE.2yr[order(CE.2yr$yld.rank),]
CF.2yr<-CF.2yr[order(CF.2yr$yld.rank),]
SE.2yr<-SE.2yr[order(SE.2yr$yld.rank),]
SF.2yr<-SF.2yr[order(SF.2yr$yld.rank),]
```


# District Summary Stats Tables

## North Early (1A)

```{r NE.District.Stats.Table, echo=FALSE}
## North Early
## District Yield
NE.yMean<-mean(NE.LSMy$ls.yld)
NE.ylsd<-predictmeans(NE.ymodel, "code", pairwise=TRUE, level=0.25, pplot=FALSE, mplot=FALSE, newwd=FALSE)
NE.yLSD<-NE.ylsd[["LSD"]][["Aveg.LSD"]]
## Moisture
NE.mMean<-mean(NE.LSMm$ls.moist)
NE.mlsd<-predictmeans(NE.mmodel, "code", pairwise=TRUE, level=0.25, pplot=FALSE, mplot=FALSE, newwd=FALSE)
NE.mLSD<-NE.mlsd[["LSD"]][["Aveg.LSD"]]
## West Yield
NE.wyMean<-mean(NE.LSMyw$ls.yld.w)
NE.wylsd<-predictmeans(NE.ymodelw, "code", pairwise=TRUE, level=0.25, pplot=FALSE, mplot=FALSE, newwd=FALSE)
NE.wyLSD<-NE.wylsd[["LSD"]][["Aveg.LSD"]]
## East Yield
NE.eyMean<-mean(NE.LSMye$ls.yld.e)
NE.eylsd<-predictmeans(NE.ymodelw, "code", pairwise=TRUE, level=0.25, pplot=FALSE, mplot=FALSE, newwd=FALSE)
NE.eyLSD<-NE.eylsd[["LSD"]][["Aveg.LSD"]]
## Root LDG
NE.rtldg.Mean<-mean(NE.ldg$NE.rtldg)
NE.rtldg.lsd<-predictmeans(model.N_Early.rtldg, "code", pairwise=TRUE, level=0.25, pplot=FALSE, mplot=FALSE, newwd=FALSE)
NE.rtldg.LSD<-NE.rtldg.lsd[["LSD"]][["Aveg.LSD"]]
## Stalk LDG
NE.skldg.Mean<-mean(NE.ldg$NE.skldg)
NE.skldg.lsd<-predictmeans(model.N_Early.skldg, "code", pairwise=TRUE, level=0.25, pplot=FALSE, mplot=FALSE, newwd=FALSE)
NE.skldg.LSD<-NE.skldg.lsd[["LSD"]][["Aveg.LSD"]]


## COMBO Stats Table:
NE.Combo.Stats<- data.frame("Value" = c("Experiment Mean", "LSD(0.25)") ,"yldM" = as.numeric(c(NE.yMean, NE.yLSD)), "moistM" = as.numeric(c(NE.mMean, NE.mLSD)), "w.yld" = as.numeric(c(NE.wyMean, NE.wyLSD)), "e.yld" = as.numeric(c(NE.eyMean, NE.eyLSD)), "rtldgM" = as.numeric(c(NE.rtldg.Mean,NE.rtldg.LSD)), "skldgM" = as.numeric(c(NE.skldg.Mean, NE.skldg.LSD)))


## Add Blank Columns:
NE.Combo.Stats<-data.frame(append(NE.Combo.Stats, list(hybrid= ""), after =  1))
NE.Combo.Stats<-data.frame(append(NE.Combo.Stats, list(rm= ""), after =  2))
NE.Combo.Stats<-data.frame(append(NE.Combo.Stats, list(pkg= ""), after =  3))
NE.Combo.Stats<-data.frame(append(NE.Combo.Stats, list(herb= ""), after =  4))
NE.Combo.Stats<-data.frame(append(NE.Combo.Stats, list(nw.yld= ""), after =  6))
NE.Combo.Stats<-data.frame(append(NE.Combo.Stats, list(AGV= ""), after =  12))
```

