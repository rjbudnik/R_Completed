CF.F5.lsd<-predictmeans(C_Full.model5, "entry", pairwise=TRUE, level=0.25, pplot=FALSE, mplot=FALSE, newwd=FALSE)
CF.F5.LSD<-CF.F5.lsd[["LSD"]][["Aveg.LSD"]]

## COMBO Stats Table:
CF.Combo.Stats<- data.frame("Value" = c("Experiment Mean",	"Minimum Mean",	"Maximum Mean",	"LSD(0.25)", 	"Coefficient of Variability") ,"yldM" = as.numeric(c(CF.Combo.Mean, CF.Combo.Min, CF.Combo.Max, CF.Combo.LSD, CF.Combo.CV)), "moistM" = as.numeric(c(CF.Moist.Mean, CF.Moist.Min, CF.Moist.Max, CF.Moist.LSD, "")), "rtldgM" = as.numeric(c(CF.rtldg.Mean, CF.rtldg.Min, CF.rtldg.Max, CF.rtldg.LSD, "")), "skldgM" = as.numeric(c(CF.skldg.Mean, CF.skldg.Min, CF.skldg.Max, CF.skldg.LSD, "")), "AGVM" = as.numeric(c(CF.AGV.Mean, CF.AGV.Min, CF.AGV.Max, "", "")), "F2M" = as.numeric(c(CF.F2.Mean, CF.F2.Min, CF.F2.Max, CF.F2.LSD, CF.F2.CV)), "F3M" = as.numeric(c(CF.F3.Mean, CF.F3.Min, CF.F3.Max, CF.F3.LSD, CF.F3.CV)), "F4M" = as.numeric(c(CF.F4.Mean, CF.F4.Min, CF.F4.Max, CF.F4.LSD, CF.F4.CV)), "F5M" = as.numeric(c(CF.F5.Mean, CF.F5.Min, CF.F5.Max, CF.F5.LSD, CF.F5.CV)))
## Round Digits:
CF.Combo.Stats$moistM<-as.numeric(CF.Combo.Stats$moistM)
CF.Combo.Stats$AGVM<-as.numeric(CF.Combo.Stats$AGVM)
CF.Combo.Stats%>%mutate_if(is.numeric, round, digits=2)
## Add Blank Columns:
CF.Combo.Stats<-data.frame(append(CF.Combo.Stats, list(hybrid= ""), after =  1))
CF.Combo.Stats<-data.frame(append(CF.Combo.Stats, list(pkg= ""), after =  2))
CF.Combo.Stats<-data.frame(append(CF.Combo.Stats, list(herb= ""), after =  3))
CF.Combo.Stats<-data.frame(append(CF.Combo.Stats, list(dist.rank= ""), after =  5))
CF.Combo.Stats<-data.frame(append(CF.Combo.Stats, list(nw.yld= ""), after =  6))
CF.Combo.Stats<-data.frame(append(CF.Combo.Stats, list(ne.yld= ""), after =  7))
CF.Combo.Stats<-data.frame(append(CF.Combo.Stats, list(F1M= ""), after =  12))
```

