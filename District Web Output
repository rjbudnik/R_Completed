# EXPORT!

```{r Export.to.Formatted.Excel, comment=NA, echo=FALSE, message=FALSE}
wb<-loadWorkbook("District_Corn_Web.xlsx", create=FALSE)
setStyleAction(wb,XLC$"STYLE_ACTION.NONE")

## Write NORTH
## Write North Early
writeWorksheet(wb, N.E.Combo, "C1A", startRow=5, startCol=1, header=FALSE)
writeWorksheet(wb, NE.Combo.Stats, "C1A", startRow=50, startCol=1, header=FALSE)
## Write North Full
writeWorksheet(wb, N.F.Combo, "C1B", startRow=5, startCol=1, header=FALSE)
writeWorksheet(wb, NF.Combo.Stats, "C1B", startRow=51, startCol=1, header=FALSE)
## Write CENTRAL
## Write Central Early
writeWorksheet(wb, C.E.Combo, "C2A", startRow=5, startCol=1, header=FALSE)
writeWorksheet(wb, CE.Combo.Stats, "C2A", startRow=58, startCol=1, header=FALSE)
## Write Central Full
writeWorksheet(wb, C.F.Combo, "C2B", startRow=5, startCol=1, header=FALSE)
writeWorksheet(wb, CF.Combo.Stats, "C2B", startRow=58, startCol=1, header=FALSE)
## Write SOUTH
## Write South Early
writeWorksheet(wb, S.E.Combo, "C3A", startRow=5, startCol=1, header=FALSE)
writeWorksheet(wb, SE.Combo.Stats, "C3A", startRow=50, startCol=1, header=FALSE)
## Write South Full
writeWorksheet(wb, S.F.Combo, "C3B", startRow=5, startCol=1, header=FALSE)
writeWorksheet(wb, SF.Combo.Stats, "C3B", startRow=42, startCol=1, header=FALSE)

saveWorkbook(wb)
```
