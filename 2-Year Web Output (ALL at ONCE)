# EXPORT!

```{r Export.to.Formatted.Excel, comment=NA, echo=FALSE, message=FALSE}
wb<-loadWorkbook("Corn_2Yr_Web.xlsx", create=FALSE)
setStyleAction(wb,XLC$"STYLE_ACTION.NONE")

## Write NORTH
## Write North Early
writeWorksheet(wb, NE.2yr, "North", startRow=6, startCol=1, header=FALSE)
writeWorksheet(wb, NE.Combo.Stats, "North", startRow=26, startCol=1, header=FALSE)
## Write North Full
writeWorksheet(wb, NF.2yr, "North", startRow=28, startCol=1, header=FALSE)
writeWorksheet(wb, NF.Combo.Stats, "North", startRow=45, startCol=1, header=FALSE)

## Write CENTRAL
## Write Central Early
writeWorksheet(wb, CE.2yr, "Central", startRow=6, startCol=1, header=FALSE)
writeWorksheet(wb, CE.Combo.Stats, "Central", startRow=25, startCol=1, header=FALSE)
## Write Central Full
writeWorksheet(wb, CF.2yr, "Central", startRow=28, startCol=1, header=FALSE)
writeWorksheet(wb, CF.Combo.Stats, "Central", startRow=54, startCol=1, header=FALSE)

## Write SOUTH
## Write South Early
writeWorksheet(wb, SE.2yr, "South", startRow=6, startCol=1, header=FALSE)
writeWorksheet(wb, SE.Combo.Stats, "South", startRow=24, startCol=1, header=FALSE)
## Write South Full
writeWorksheet(wb, SF.2yr, "South", startRow=27, startCol=1, header=FALSE)
writeWorksheet(wb, SF.Combo.Stats, "South", startRow=38, startCol=1, header=FALSE)


saveWorkbook(wb)
```
